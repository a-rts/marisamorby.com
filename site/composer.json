{
  "name": "marisamorby.com",
  "type": "project",
  "license": "ISC",
  "description": "Marisa Morby's 2016 website.",
  "homepage": "http://marisamorby.com/",
  "authors": [
    {
      "name": "Jason Lengstorf",
      "email": "jason@lengstorf.com",
      "homepage": "https://github.com/jlengstorf"
    }
  ],
  "keywords": [],
  "support": {
    "issues": "https://github.com/jlengstorf/marisamorby.com/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.3.7",
        "type": "wordpress-muplugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
        },
        "require": {
          "philippbaschke/acf-pro-installer": "^1.0",
          "composer/installers": "^1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "gravityforms/gravityforms",
        "version": "1.9.19",
        "type": "wordpress-muplugin",
        "dist": {
          "url": "./uncooperative_plugins/gravityforms_1.9.19.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.5.2",
    "oscarotero/env": "^1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "yoast/wordpress-seo": "*",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "roots/soil": "*",
    "wpackagist-plugin/easy-footnotes": "*",
    "wpackagist-plugin/google-analytics-dashboard-for-wp": "*",
    "wpackagist-plugin/highlight-and-share": "*",
    "afragen/github-updater": "*",
    "wp-sync-db/wp-sync-db": "dev-master@dev",
    "wp-sync-db/wp-sync-db-media-files": "*",
    "wp-api/wp-api": "*@dev",
    "wpackagist-plugin/acf-to-wp-api": "*",
    "mailgun/mailgun-php": "dev-master#89869402e04d1ff783a99fd654ac10d8e4e4f136",
    "php-http/guzzle6-adapter": "1.0",
    "gravityforms/gravityforms": "1.9.19"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin",
        "roots/soil",
        "wp-api/wp-api",
        "wpackagist-plugin/acf-to-wp-api"
      ],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs --ignore=web/wp/,vendor/ -n -s ."
    ]
  }
}
